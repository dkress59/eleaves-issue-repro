{
	"name": "eleaves",
	"version": "1.0.0",
	"description": "the eleaves platform of the alex healthcare gmbh",
	"main": "index.js",
	"author": "dkress59 <28515387+dkress59@users.noreply.github.com>",
	"license": "MIT",
	"private": true,
	"packageManager": "yarn@3.2.2",
	"scripts": {
		"build": "yarn css:email && yarn build:server && yarn build:dashboard",
		"build:all": "yarn css:email && yarn workspaces foreach --parallel run build",
		"css:email": "yarn postcss ./apps/_common/src/css/email.scss -o ./apps/_server/src/mail/email.css",
		"check:script": "eslint --fix-dry-run .",
		"check:style": "stylelint '**/*.scss'",
		"fix:script": "eslint --fix .",
		"fix:style": "stylelint --fix --quiet '**/*.scss'",
		"start": "yarn start:dashboard",
		"test": "jest --verbose",
		"test:apps": "jest --selectProjects apps",
		"test:server": "jest --selectProjects server:e2e server:units",
		"test:watch": "jest --watchAll",
		"dump:postgres": "docker exec eleaves-postgres pg_dump -Ft -C -U directus -f /home/dump.db directus && docker cp eleaves-postgres:/home/dump.db ./dump-\"$(date '+%F')\"-\"$(date '+%s')\".db",
		"postinstall": "husky install",
		"storybook": "start-storybook -p 6006 --no-open --quiet",
		"build:storybook": "build-storybook -o build/_storybook --quiet",
		"serve:storybook": "npx http-server build/_storybook -p 6006",
		"update:humbee": "./update-humbee-specs.sh",
		"update:sdks": "yarn dlx @yarnpkg/sdks",
		"m1:install-canvas-deps": "brew install pkg-config cairo pango libpng jpeg giflib librsvg"
	},
	"workspaces": [
		"apps/*"
	],
	"lint-staged": {
		"*.{json,js,jsx,ts,tsx,scss}": [
			"yarn prettier --write"
		],
		"*.{scss, css}": [
			"yarn stylelint --fix --quiet"
		]
	},
