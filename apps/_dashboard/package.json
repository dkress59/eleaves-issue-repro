{
	"name": "dashboard",
	"version": "1.0.0",
	"packageManager": "yarn@3.2.1",
	"scripts": {
		"build": "webpack --mode production && echo 'dashboard successfully built.'",
		"build:dashboard": "yarn build",
		"start": "webpack serve --mode development",
		"start:dashboard": "yarn start",
		"start:dashboard:prod": "npx serve -s ../../build/_dashboard"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"common": "workspace:apps/_common",
		"demo-app-1": "workspace:apps/demo-app-1",
		"demo-app-2": "workspace:apps/demo-app-2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-loading-icons": "1.1.0",
		"react-router": "6.3.0",
		"react-router-dom": "6.3.0"
	},
	"devDependencies": {
		"@types/node": "18.7.16",
		"@types/react": "18.0.19",
		"@types/react-dom": "18.0.6",
		"clean-webpack-plugin": "4.0.0",
		"copy-webpack-plugin": "11.0.0",
		"css-loader": "6.7.1",
		"html-webpack-plugin": "5.5.0",
		"postcss": "8.4.16",
		"postcss-loader": "7.0.1",
		"sass": "1.54.9",
		"sass-loader": "13.0.2",
		"style-loader": "3.3.1",
		"ts-loader": "9.3.1",
		"ts-node": "10.9.1",
		"typescript": "4.7.4",
		"webpack": "5.74.0",
		"webpack-cli": "4.10.0",
		"webpack-dev-server": "4.11.0"
	}
}
