{
	"name": "demo-app-1",
	"version": "1.0.2",
	"private": true,
	"type": "commonjs",
	"packageManager": "yarn@3.1.1",
	"scripts": {
		"css": "yarn postcss --env=production src/index.scss -o ../../build/demo-app-1/index.css",
		"build": "webpack --mode=production --progress && echo 'demo-app-1 successfully built.'",
		"build:demo1": "yarn build",
		"start": "webpack serve --port 3000 --mode development",
		"start:demo1": "yarn start"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"common": "workspace:apps/_common",
		"moment": "2.29.4",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@types/node": "18.7.16",
		"@types/postcss-import": "14.0.0",
		"@types/react": "18.0.19",
		"@types/react-dom": "18.0.6",
		"autoprefixer": "10.4.9",
		"clean-webpack-plugin": "4.0.0",
		"copy-webpack-plugin": "11.0.0",
		"css-loader": "6.7.1",
		"html-webpack-plugin": "5.5.0",
		"postcss": "8.4.16",
		"postcss-cli": "10.0.0",
		"postcss-import": "15.0.0",
		"postcss-loader": "7.0.1",
		"sass": "1.54.9",
		"sass-loader": "13.0.2",
		"style-loader": "3.3.1",
		"tailwindcss": "3.1.8",
		"ts-loader": "9.3.1",
		"ts-node": "10.9.1",
		"typescript": "4.7.4",
		"webpack": "5.74.0",
		"webpack-cli": "4.10.0",
		"webpack-dev-server": "4.11.0"
	}
}
